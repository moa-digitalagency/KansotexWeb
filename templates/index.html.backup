                            <span>Maroc</span>
                        </div>
                    </div>
                </div>

                <!-- Contact Form -->
                <div class="p-8 rounded-lg border" style="background: #0a0a0a; border-color: rgba(212, 175, 55, 0.3);">
                    <h3 class="text-2xl font-bold mb-6">Contactez-nous</h3>
                    <form id="contact-form" class="space-y-4">
                        <div>
                            <input type="text" 
                                   id="name" 
                                   name="name" 
                                   placeholder="Votre nom *" 
                                   required
                                   class="w-full px-4 py-3 rounded-lg focus:outline-none transition"
                                   style="background: #141414; border: 1px solid rgba(100, 100, 100, 0.3);">
                        </div>
                        <div>
                            <input type="email" 
                                   id="email" 
                                   name="email" 
                                   placeholder="Votre email *" 
                                   required
                                   class="w-full px-4 py-3 rounded-lg focus:outline-none transition"
                                   style="background: #141414; border: 1px solid rgba(100, 100, 100, 0.3);">
                        </div>
                        <div>
                            <input type="tel" 
                                   id="phone" 
                                   name="phone" 
                                   placeholder="Votre téléphone" 
                                   class="w-full px-4 py-3 rounded-lg focus:outline-none transition"
                                   style="background: #141414; border: 1px solid rgba(100, 100, 100, 0.3);">
