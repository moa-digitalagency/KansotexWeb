modules = ["web", "python-3.12", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "flask-server"

[[workflows.workflow]]
name = "flask-server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34255
externalPort = 3000

[[ports]]
localPort = 37625
externalPort = 3002

[[ports]]
localPort = 37873
externalPort = 3003

[[ports]]
localPort = 43213
externalPort = 4200

[[ports]]
localPort = 44071
externalPort = 3001

[nix]
channel = "stable-25_05"
packages = ["freetype", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]
